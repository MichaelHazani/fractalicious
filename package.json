{
  "name": "three-quickstart",
  "version": "1.0.0",
  "description": "A Stripped Down, Modern ThreeJS Boilerplate",
  "main": "./src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "watchify src/app.js -o js/app.js -t [ babelify --presets [ es2015 ] ]",
    "serve": "browser-sync start --server  --files '*.html, ./src/*.js' --reloadDelay 1000 --no-notify",
    "go": "concurrently --kill-others \"npm run start\" \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelHazani/threejs-quickstart.git"
  },
  "keywords": [
    "threejs",
    "boilerplate",
    "node",
    "webgl",
    "3D",
    "es6",
    "babel"
  ],
  "author": "Michael Hazani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelHazani/threejs-quickstart/issues"
  },
  "homepage": "https://github.com/MichaelHazani/threejs-quickstart#readme",
  "dependencies": {
    "three": "^0.85.2",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browsersync": "0.0.1-security",
    "concurrently": "^3.4.0",
    "watchify": "^3.9.0"
  }
}